{
    "env": {
        "browser": false,
        "es6": true,
        "node": true
    },
    "extends": ["eslint: recommend", "prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "prettier/prettier": "error",
        "camelcase": "error"
    }
}
